{
  "name": "open-interest-history",
  "description": "从币安永续合约获取历史持仓量(Open Interest)数据，显示市场未平仓合约数量的变化趋势",
  "parameters": [
    {
      "name": "symbol",
      "type": "string",
      "displayName": "交易对",
      "description": "币安交易对符号，如 BTCUSDT",
      "required": true,
      "validation": {
        "pattern": "^[A-Z]{2,20}USDT$"
      }
    },
    {
      "name": "period",
      "type": "enum",
      "displayName": "时间周期",
      "description": "持仓量数据的时间聚合周期",
      "required": true,
      "defaultValue": "5m",
      "validation": {
        "enum": ["5m", "15m", "30m", "1h", "2h", "4h", "6h", "12h", "1d"]
      }
    },
    {
      "name": "limit",
      "type": "number",
      "displayName": "数据条数",
      "description": "获取的数据条数，最大500",
      "required": false,
      "defaultValue": 100,
      "validation": {
        "min": 1,
        "max": 500
      }
    },
    {
      "name": "endTime",
      "type": "number",
      "displayName": "结束时间戳",
      "description": "结束时间的毫秒时间戳，不填则使用当前时间",
      "required": false,
      "defaultValue": 0,
      "validation": {
        "min": 0
      }
    }
  ]
}
